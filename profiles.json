  {
    "id": "blinklocal",
    "type": "bin",
    "device": "m328p",
    "programmer": "linuxspi",
    "jumper" : 2,
    "bins": [
      {
        "method": "fs",
        "name": "blinklocal",
        "src": "sketches/cloudblink/cloudblink.ino.standard.hex"
      }
    ],
    "fuses": {
      "lfuse": "0xF7",
      "hfuse": "0xD6",
      "efuse": "0xFD",
      "lock": "0xFF"
    },
    "plugins": [
      {
        "name": "serialinjector",
        "conf": {
          "serialSpeed": 9600,
          "fields": [
            {
              "id": "blinkrate",
              "value": 5,
              "title": "Blink rate in 100ms. For exampe value of 5 means 500ms off, 500ms on",
              "type": "byte"
            },
            {
              "id": "intensity",
              "value": 100,
              "title": "LED intensity (0-255)",
              "type": "byte"
            }
          ]
        }
      }
    ]
  },
